<h1>
  <strong>Books</strong>
</h1>

<!--ここから表　※テーブルにもヘッダやボディとかがある-->
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Body</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  
  <tbody>
    <% @books.each do |book| %>
    <tr>
      <td><%= books.title %></td>
      <td><%= books.body %></td>
      <td>
        <%= link_to "show",post_path(book.id) %>
        <%= link_to "edit",post_path(book.id) %>
      </td>
    </tr>
  </tbody>
</table>